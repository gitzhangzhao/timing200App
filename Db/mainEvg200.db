#        field(MOD2, "Alternate")
field(MOD1,"Off")
field(MOD2, "Off")
field(R1SP, "0")
field(R2SP, "0")
field(FIFO, "NO")
field(ENAB, "YES")
field(ETE0, "NO")
field(ET0, "0")
field(ETE1, "NO")
field(ET1, "0x0")
field(ETE2, "NO")
field(ET2, "0x0")
field(ETE3, "YES")
field(ET3, "0x1")
field(ETE4, "YES")
field(ET4, "0x7c")
field(ETE5, "NO")
field(ET5, "0x0")
field(ETE6, "NO")
field(ET6, "0x0")
field(ETE7, "NO")
field(ET7, "0x0")
field(ME4E, "YES")
field(MC4P, "50000")
field(ME3E, "YES")
field(MC3P, "50000000")
#       field(VME, "0x18")
        field(ADIV, "1")
        field(AEV0, "NO")
        field(R1CK, "6")
        field(R2CK, "6")
#       field(R1SP,"50000000")
#       field(R2SP,"50000000")
        field(MD6E, "YES")
        field(MC6P, "196")
        field(MD7E, "YES")
        field(MC7P, "1384")
        field(ASYN, "YES")
        field(ASQ1, "YES")
        field(ASQ2, "YES")

}

