

record(eg, "$(SUBSYS):$(DEVICE):eg")
{
field(DESC, "BEPCIItimingMainEVG")
field(ASG, "")
field(SCAN, "Passive")
field(PINI, "YES")
field(PHAS, "0")
field(EVNT, "0")
field(TSE, "0")
field(TSEL, "0")
field(DTYP, "APS event generator G")
field(DISV, "1")
field(SDIS, "0")
field(DISS, "NO_ALARM")
field(PRIO, "LOW")
	field(FLNK, "0")
	field(OUT, "#C$(EGslot) S0 @")
